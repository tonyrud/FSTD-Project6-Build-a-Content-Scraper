{
    "env": {
        "browser": false,
        "commonjs": false,
        "es6": true,
        "node": true,
        "jquery": false
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "one-var": 0,
        "indent": 0,
        "no-else-return": 0,
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}